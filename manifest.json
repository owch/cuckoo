{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["core.css"],
      
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}